{
	"projection":"Orthographic",
	"geodata":"worldLow",
	"panBehavior":"rotateLongLat",
	"deltaLongitude":20,
	"deltaLatitude":-20,
	"padding":[
		20,
		20,
		20,
		20
	],
	"geodataNames":"",
	"hiddenState":{
		"properties":{
			"opacity":0
		}
	},
	"adapter":{
		"deltaLatitude":{}
	},
	"backgroundSeries":{
		"mapPolygons":{
			"template":{
				"polygon":{
					"fillOpacity":0,
					"fill":"#fff"
				}
			}
		}
	},
	"series":[
		{
			"type":"MapPolygonSeries",
			"useGeodata":true,
			"mapPolygons":{
				"template":{
					"tooltipText":"{name}",
					"polygon":{
						"fill":"#cccccc",
						"stroke":"#454a58",
						"strokeWidth":0.5
					},
					"states":{
						"hover":{
							"properties":{
								"fill":"#aaaaaa",
								"fillOpacity":0.8
							}
						}
					}
				}
			}
		},
		{
			"type":"GraticuleSeries",
			"mapLines":{
				"template":{
					"line":{
						"stroke":"#000",
						"strokeWidth":0.4,
						"strokeOpacity":0.3
					}
				}
			},
			"fixExtent":true
		},
		{
			"type":"MapImageSeries",
			"mapImages":{
				"template":{
					"propertyFields":{
						"longitude":"longitude",
						"latitude":"latitude",
						"url":"url"
					},
					"tooltipText":"{title}",
					"children":[
						{
							"type":"Circle",
							"radius":3,
							"propertyFields":{
								"fill":"color"
							}
						},
						{
							"type":"Circle",
							"radius":3,
							"propertyFields":{
								"fill":"color"
							},
							"events":{
								"inited":{

								}
							}
						}
					]
				}
			},
			"data":[

			]
		}
	],
	"legend":{
		"maxHeight":150,
		"maxWidth":300,
		"scrollable":true
	},
	"exporting":{
		"menu":{

		}
	},
	"zoomControl":{

	}
}