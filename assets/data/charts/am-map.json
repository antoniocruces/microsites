{
	"projection":"Miller",
	"geodata":"worldLow",
	"geodataNames":"",
	"hiddenState":{
		"properties":{
			"opacity":0
		}
	},
	"series":[
		{
			"type":"MapPolygonSeries",
			"exclude":["AQ"],
			"useGeodata":true,
			"mapPolygons":{
				"template":{
					"tooltipText":"{name}",
					"polygon":{
						"fillOpacity":0.6
					},
					"states":{
						"hover":{
							"properties":{
								"fill":"#aaa",
								"fillOpacity":0.8
							}
						}
					}
				}
			}
		},
		{
			"type":"GraticuleSeries",
			"mapLines":{
				"template":{
					"line":{
						"stroke":"#000",
						"strokeWidth":0.2,
						"strokeOpacity":0.08
					}
				}
			},
			"fixExtent":false
		},
		{
			"type":"MapImageSeries",
			"mapImages":{
				"template":{
					"propertyFields":{
						"longitude":"longitude",
						"latitude":"latitude",
						"url":"url"
					},
					"tooltipText":"{title}",
					"children":[
						{
							"type":"Circle",
							"radius":3,
							"propertyFields":{
								"fill":"color"
							}
						},
						{
							"type":"Circle",
							"radius":3,
							"propertyFields":{
								"fill":"color"
							},
							"events":{
								"inited":{}
							}
						}
					]
				}
			},
			"data":[]
		}
	],
	"legend":{
		"maxHeight":150,
		"maxWidth":300,
		"scrollable":true
	},
	"exporting":{
		"menu":{}
	},
	"zoomControl":{}
}